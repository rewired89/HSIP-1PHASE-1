{
  "manifest_version": 2,
  "name": "HSIP Consent Demo",
  "version": "0.1",
  "description": "Request HSIP consent tokens from HSIP tray.",
  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "HSIP Login"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": [
    "http://127.0.0.1:9389/*"
  ]
}
